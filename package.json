{
  "name": "more-prettier",
  "description": "CLI for running [prettier] with opinionated defaults.",
  "version": "0.1.0",
  "homepage": "https://github.com/jonschlinkert/more-prettier",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/more-prettier",
  "bugs": {
    "url": "https://github.com/jonschlinkert/more-prettier/issues"
  },
  "license": "MIT",
  "files": [
    "cli.js"
  ],
  "main": "cli.js",
  "preferGlobal": true,
  "bin": {
    "fix": "cli.js"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "composer": "^0.14.0",
    "extend-shallow": "^2.0.1",
    "minimist": "^1.2.0",
    "object.omit": "^2.0.1",
    "prettier": "^1.3.1",
    "through2": "^2.0.3",
    "vinyl-fs": "^2.4.4"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.12"
  },
  "keywords": [
    "fix",
    "js",
    "javascript",
    "format",
    "formatter",
    "prettify",
    "beautify",
    "beautifier",
    "prettier"
  ],
  "lintDeps": {
    "dependencies": {
      "files": [
        "*.js"
      ]
    }
  },
  "verb": {
    "layout": "default",
    "include": {
      "install-npm": "install-global"
    },
    "plugins": [
      "gulp-format-md"
    ],
    "toc": false,
    "tasks": [
      "readme"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
